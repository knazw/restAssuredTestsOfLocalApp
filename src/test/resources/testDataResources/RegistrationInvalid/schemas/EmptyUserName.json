{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "uuid": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "balance": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string"
        },
        "modifiedAt": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "uuid",
        "firstName",
        "lastName",
        "password",
        "balance",
        "createdAt",
        "modifiedAt"
      ]
    }
  },
  "required": [
